export TF_VAR_aws_secret_key="your-aws-secret-key"

# PostgreSQL Sensitive Data
export TF_VAR_postgres_password="your-postgres-password"

# JWT Sensitive Data
export TF_VAR_jwt_secret="your-jwt-secret-key"

